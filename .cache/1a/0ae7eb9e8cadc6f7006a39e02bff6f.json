ng: 0;\n  z-index: 2;\n  width: 100%;\n  // text-align: justify;\n\n  // &:last-of-type {\n  //   padding-bottom: 30em;\n  // }\n}\n\nimg {\n  position: fixed;\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%) scale(1.1);\n  max-width: 600px;\n  max-height: 80vh;\n  height: auto;\n  opacity: 0;\n  transition: all .5s ease-in-out;\n  z-index: 0;\n  transform-origin: center;\n  pointer-events: none;\n\n  &:first-of-type {\n    left: 20%;\n  }\n\n  &:nth-of-type(2) {\n    left: 70%;\n  }\n\n  &:last-of-type {\n    left: 70%;\n  }\n\n  &.active {\n    transform: translate(-50%, -50%) scale(1);\n    opacity: 1;\n  }\n}\n\n.opening {\n  position: fixed;\n  top: 0;\n  left: 0;\n  height: 100vh;\n  width: 100vw;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-color: #092D23;\n  z-index: 9999;\n  transition: top .7s cubic-bezier(0.83, 0, 0.17, 1);\n\n  &.hidden {\n    top: -101vh;\n    pointer-events: none;\n  }\n\n  h1 {\n    color: #fff;\n    font-size: 5em;\n  }\n  \n  button {\n    position: relative;\n    display: block;\n    margin: 2em auto;\n    padding: 1em 2em;\n    background-color: transparent;\n    border: solid 1px #fff;\n    color: #fff;\n    text-transform: uppercase;\n    letter-spacing: 1px;\n    font-weight: 400;\n    overflow: hidden;\n    cursor: pointer;\n\n    &::after {\n      content: '';\n      position: absolute;\n      left: 0;\n      top: 100%;\n      width: 100%;\n      height: 100%;\n      background-color: #fff;\n      transition: top .3s cubic-bezier(0.83, 0, 0.17, 1), color .15s ease-in-out .25s;\n      z-index: -1;\n    }\n\n    &:hover {\n      color: #000;\n      outline: none;\n\n      &::after {\n        top: 0;\n      }\n    }\n  }\n}\n\nh1 span {\n  background:\n    linear-gradient(#fff,#fff) left no-repeat,\n   #092D23;\n  -webkit-background-clip: text;\n  background-clip: text;\n  -webkit-text-fill-color: transparent;\n  color: transparent;\n  animation:loading 68s forwards linear;\n  z-index: 99;\n}\n\n.fill {\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  h1 {\n    color: #092D23;\n    text-align: center;\n  }\n}\n\n.mute-button {\n  position: fixed;\n  right: 1em;\n  bottom: 1em;\n  width: 70px;\n  height: 70px;\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: center;\n  background-color: #101010;\n  border-radius: 50%;\n  cursor: pointer;\n  z-index: 99;\n\n  &.muted {\n    .line {\n      width: 60%;\n    }\n\n    svg {\n      opacity: 0;\n    }\n  }\n\n  svg {\n    position: absolute;\n    height: 40px;\n    width: 40px;\n    fill: #fff;\n    transition: all .3s ease-in-out;\n\n    @for $i from 1 through 9 {\n      path:nth-of-type(#{$i}) {\n        animation: pulse 1s infinite;\n        animation-delay: $i * .15s;\n      }\n    }\n  }\n\n  .line {\n    width: 0;\n    height: 3px;\n    background-color: #fff;\n    border-radius: 50px;\n    transition: all .3s ease-in-out;\n  }\n}\n\n@keyframes pulse {\n\t0% {\n\t\ttransform: scaleY(1);\n\t\ttransform-origin: 50% 50%;\n\t}\n\t\n\t50% {\n\t\ttransform: scaleY(.7);\n\t\ttransform-origin: 50% 50%;\n\t}\n\t\n\t100% {\n\t\ttransform: scaleY(1);\n\t\ttransform-origin: 50% 50%;\n\t}\n}"},"lineCount":null}},"error":null,"hash":"9c03ba801d84ba058df5e106d5c1225d","cacheData":{"env":{}}}